{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": ["class-property"],
  "rules": {
    "no-undef": 0,
    "react/prop-types": 0,
    "react/button-has-type": 0,
    "react/jsx-one-expression-per-line": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/label-has-for": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0
  },
  "globals": {
    "API_URL": true,
    "fixture": true
  }
}
